## Rename this file to .env and set your secrets locally.

# Gemini LLM (optional)
GEMINI_API_KEY=
# Prefer LLM summary by default (true/false)
SUMMARY_LLM_DEFAULT=false

# Chrome/Chromium paths (optional, for Lighthouse/Puppeteer)
# CHROME_PATH=/usr/bin/google-chrome
# PUPPETEER_EXECUTABLE_PATH=/usr/bin/google-chrome

# Journey flows (optional)
# FLOWS_PATH=/absolute/path/to/flows.json

# Server port
# PORT=8787

